<event
{{#if canClickEvents}}
  onClick="location.href='{{eventUrl id useApplicationUrl}}';"
  class="pointer"
{{/if}}
>
  {{#if event.series}}
    <left>
      <series>{{event.series.title}}</series>
      <img src="{{bannerUrl event.series.id useApplicationUrl}}">
    </left>
  {{/if}}
  <data>
    <detail>
    {{> (eventPartial event) this}}
    </detail>
    <time>{{dateTime timestamp}}</time>
  </data>
</event>