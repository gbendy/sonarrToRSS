<?xml version="1.0"?>
<Container version="2">
  <Name>sonarrtorss</Name>
  <Repository>gbendy/sonarrtorss</Repository>
  <Registry>https://registry.hub.docker.com/r/gbendy/sonarrtorss/</Registry>
  <Network></Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/gbendy/sonarrToRSS/issues/new</Support>
  <Project>https://github.com/gbendy/sonarrToRSS</Project>
  <Overview>Sonarr To RSS is a Webhook connection for Sonarr that provides events via an RSS feed and paginated website.</Overview>
  <Category/>
  <WebUI>http://[IP]:[PORT:18989]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/gbendy/sonarrToRSS/main/templates/unraid/SonarrToRss.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/gbendy/sonarrToRSS/main/img/sonarrToRSS.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="WebUI" Target="18989" Default="18989" Mode="tcp" Description="The port for the Sonarr to RSS webinterface" Type="Port" Display="always" Required="true" Mask="false">18989</Config>
  <Config Name="Appdata" Target="/data" Default="/mnt/user/appdata/sonarrtorss" Mode="rw" Description="Database and sonarr to rss configs" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/sonarrtorss</Config>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">99</Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
</Container>
