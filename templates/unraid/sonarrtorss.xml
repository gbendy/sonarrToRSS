<?xml version="1.0"?>
<Container version="2">
  <Name>sonarrtorss</Name>
  <Repository>gbendy/sonarrtorss</Repository>
  <Registry>https://hub.docker.com/r/gbendy/sonarrtorss/</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/gbendy/sonarrToRSS/issues/new</Support>
  <Project>https://github.com/gbendy/sonarrToRSS</Project>
  <Overview>Sonarr To RSS is a Sonarr Webhook connection endpoint that streams events to RSS/Atom/JSON feeds and provides a paginated website to browse historical events.</Overview>
  <Category>Downloaders: MediaApp:Other</Category>
  <WebUI>http://[IP]:[PORT:18989]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/gbendy/sonarrToRSS/main/templates/unraid/sonarrtorss.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/gbendy/sonarrToRSS/main/img/sonarrToRSS.png</Icon>
  <Config Name="Appdata" Target="/data" Default="/mnt/user/appdata/sonarrtorss" Mode="rw" Description="Database and Sonarr to RSS configs" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/sonarrtorss</Config>
  <Config Name="WebUI" Target="18989" Default="18989" Mode="tcp" Description="The port for the Sonarr to RSS web interface" Type="Port" Display="always" Required="true" Mask="false">18989</Config>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">100</Config>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">99</Config>
</Container>
